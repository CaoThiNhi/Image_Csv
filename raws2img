#!/bin/bash

## declare an db file array
declare -a arr=("04015" "04043" "04048" "04126" "04746" "04908" "04936" "05091" "05121" "05261" "06426" "06453" "06995" "07162" "07859" "07879" "07910" "08215" "08219" "08378" "08405" "08434" "08455" "16265" "16272" "16273" "16420" "16483" "16539" "16773" "16786" "16795" "17052" "17453" "18177" "18184" "19088" "19090" "19093" "19140" "19830")

for i in "${arr[@]}"
do
  file=$i
  rdsamp -r raw/${file} -c -H -f 0 -t 1 -pd > csv/${file}.csv
  gnuplot -e "fileIn='csv/${file}.csv'; fileOut='uploads/${file}.png'" csv2img.gnuplot
done
